  "name": "@uplift-lunch-n-learn/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "start:metro": "nx start presenter-native",
    "start:ios": "nx pod-install presenter-native && nx run-ios presenter-native",
    "start:android": "nx run-android presenter-native",
    "start:react": "nx serve react-web",
    "wait:react": "wait-on http-get://localhost:4200",
    "start:api": "nx serve api",
    "wait:api": "wait-on http-get://localhost:5001/events",
    "build": "nx run-many --target build",
    "start:app": "concurrently --prefix-colors \"green,yellow\" --names \"api,react-web\" \"pnpm start:api\" \"pnpm start:react\"",
    "wait:app": "concurrently \"pnpm wait:api\" \"pnpm wait:react\"",
    "test:all": "nx run-many --target test",
    "e2e:react": "nx e2e web-e2e --configuration react",
    "e2e:react:watch": "pnpm e2e:react --watch",
    "e2e": "concurrently --kill-others --success \"first\" \"pnpm start:app\" \"pnpm wait:app && pnpm e2e:react\"",
    "update-nx": "nx migrate latest",
    "verify": "pnpm build && pnpm e2e",
    "verify:affected": "nx affected -t lint build test && pnpm e2e"
  },
